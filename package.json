{
    "name": "ts-key-value-cache",
    "version": "1.1.2",
    "description": "A typescript key-value cache that support time-to-live management.",
    "files": [
        "lib/**/*"
    ],
    "main": "./lib/index.js",
    "types": "./lib/api-index.d.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/tcm9439/ts-key-value-cache.git"
    },
    "scripts": {
        "test": "jest",
        "build": "tsc --project tsconfig-build.json && tsc-alias -p tsconfig-build.json",
        "build-types": "api-extractor run",
        "test-c": "jest --coverage",
        "doc": "typedoc --skipErrorChecking --entryPointStrategy expand ./src --out ./doc --theme hierarchy"
    },
    "keywords": [
        "typescript",
        "cache",
        "key value cache"
    ],
    "author": "MaisyT",
    "license": "UNLICENSED",
    "dependencies": {
        "lodash": "^4.17.21",
        "min-heap": "^0.2.3"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.33.7",
        "@types/node": "^18.11.18",
        "@types/jest": "^29.2.5",
        "@types/lodash": "^4.14.191",
        "tsc-alias": "^1.8.2",
        "typedoc": "^0.23.24",
        "typedoc-theme-hierarchy": "^3.0.2",
        "jest": "^29.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
